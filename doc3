	function sector27($j,$j1,$j2,$i)
	{
		if ($j == 1 && $j2 == 1) return 'rgb(255,128,0)';
			elseif ($j == 1 && $j1 == 1 ) return 'rgb(0,0,255)';
			elseif ($j == 0 && $j2 == 1) return	'#FF00FF';
			elseif ($j == 1 && $i == 1 ) return 'rgb(255,128,0)';
			elseif ($j == 1 && $j1 == 0 ) return 'rgb(255,128,0)';
			elseif ($j == 1 || $j2 == 1) return 'rgb(255,128,0)';
			elseif ($j == 1 && $j2 == 1) return '<polyline points="1003.14 117.15 1042.29 117.15" fill="none" class="blink" stroke-width="2" />';
			elseif ($j == 0 && $j2 == 0) return '#D3D3D3';
			else return 'red';
	}

	function sector28($j,$j1,$o,$i,$l)
	{
		if ($o == 1 && $i == 1 ) return 'rgb(255,128,0)';
			elseif ($j == 1 && $j1 == 1 ) return 'rgb(0,0,255)';
			elseif ($j == 1 && $j1 == 0 ) return 'rgb(255,128,0)';
			elseif ($j == 1) return 'rgb(255,128,0)';
			elseif ($j1 == 1 && $o == 1 && $l == 0) return 'rgb(255,128,0)';
			elseif ($j1 == 1 && $o == 1) return 'rgb(0,0,255)';
			elseif ($j == 0) return '#D3D3D3';
			else return 'red';
	}



	function sector29($j,$j1,$o,$u,$r,$i,$l)
	{
		if ($o == 1 && $j1 == 1 && $i == 1 ) return 'rgb(255,128,0)';
			elseif ($j1 == 1 && $o == 1 && $l == 0) return 'rgb(255,128,0)';
			elseif($o == 1 && $j1 == 1 ) return 'rgb(0,0,255)';
			elseif ($o == 1 || $u == 1) return 'rgb(255,128,0)';
			elseif ($u == 1 && $r == 1 ) return 'rgb(255,128,0)';
			elseif ($o == 1 && $i == 1 ) return 'rgb(255,128,0)';
			elseif ($o == 0 && $u == 0) return '#D3D3D3';
			else return 'red';
	}

	function sector30($j,$j1,$o,$u,$i,$l)
	{
		if ($o == 1 && $u == 1) return '<polyline points="1003.14 117.15 1042.29 117.15" fill="none" class="blink" stroke-width="2" />';
			elseif ($o == 1 && $j1 == 1 && $i == 1 ) return 'rgb(255,128,0)';
			elseif ($j1 == 1 && $o == 1 && $l == 0) return 'rgb(255,128,0)';
			elseif ($o == 1 && $j1 == 1 ) return 'rgb(0,0,255)';
			elseif ($u == 1 && $o == 0 ) return '#D3D3D3';
			elseif ($o == 1 && $i == 1 ) return 'rgb(255,128,0)';
			elseif ($o == 0 && $u == 0) return '#D3D3D3';
			elseif ($o == 1) return 'rgb(255,128,0)';
			else return 'red';
	}

	function sector31($r,$k)
	{
		if ($r == 0 && $k == 0) return '#D3D3D3';
			elseif ($r == 1 && $k == 1 ) return 'rgb(255,128,0)';
			elseif ($r == 1 || $k == 1 ) return 'rgb(255,128,0)';
			else return 'red';
	}

	function sector32($r,$k,$h)
	{
		if ($k == 0) return '#D3D3D3';
