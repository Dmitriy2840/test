//////////////////// Функция Задвижек
 		
function onOff($x,$y) {
	if ($x == 1 && $y == 0) return 1;
		elseif ($x == 0 && $y == 1) return 0;
		else return 2;
}

///////////////////   PVN 11 & 12!



	function pvn($pvn)
	{
		   if ($pvn == 1) return '#7FFF00';
                    elseif ($pvn == 0 ) return '#D3D3D3';
                    else return 'red';
	}

    function sector1($pvn,$a,$f,$b)
    {
           if ($pvn == 1 && $a==0 && $f==0) return '<polyline points="1003.14 117.15 1042.29 117.15" fill="none" class="blink" stroke-width="2" />';
           		elseif ($a==1 && $b==1) return 'rgb(255,128,0)';
           		elseif ($a==1 && $f==1) return 'rgb(255,128,0)';	
          		elseif ($a==1) return 'rgb(255,128,0)';
          		elseif ($f==1) return 'rgb(0,0,255)';
           		elseif ($a==0 && $f==0) return '#D3D3D3';
           		else return 'red';
    }


	function sector2($a,$f,$b,$c)
	{
           if  ($a==1 && $c==1) return '<polyline points="1003.14 117.15 1042.29 117.15" fill="none" class="blink" stroke-width="2"/>';
           		elseif ($a==1 && $f==1) return '<polyline points="1003.14 117.15 1042.29 117.15" fill="none" class="blink" stroke-width="2"/>';
           		elseif ($a==1) return 'rgb(255,128,0)';
                elseif ($a==0 && $f==1) return '#D3D3D3';
                elseif ($a==0 && $f==0) return '#D3D3D3';
                elseif ($a==1 && $f==1) return '<polyline points="1003.14 117.15 1042.29 117.15" fill="none" class="blink" stroke-width="2"/>';
                elseif ($a==1 && $b==1) return 'rgb(255,128,0)';
                else return 'red';
   	}

    function sector3($a,$c,$b)
    {
    	   if ($a == 0 && $b==1) return 'rgb(255,128,0)';
       		    elseif ($a == 1 && $b==0) return '#D3D3D3';		
                elseif ($a == 1 && $c == 0) return 'rgb(255,128,0)';
                elseif ($c == 1 && $b==0) return '#D3D3D3';
                elseif ($c == 1 && $a == 0) return 'rgb(255,128,0)';
                elseif ($a == 0 && $b==0 && $c==0) return '#D3D3D3';
                elseif ($b==1) return 'rgb(255,128,0)';
                else return 'red';
    					
   	}


$pvn12 = 0;
$pvn11 = 0;
$a = onOff($d28_7,$d28_8);;
$b = onOff($d33_11,$d33_12);
$c = onOff($d27_9,$d27_10);
$d = onOff($d27_7,$d27_8);
$e = onOff($d34_9,$d34_10);
$f = onOff($d28_5,$d28_6);


	function sector4($pvn,$c,$d,$a,$b)
	{
           if ($pvn == 1 && $c==1 && $a==1) return '<polyline points="1003.14 117.15 1042.29 117.15" fill="none" class="blink" stroke-width="2" />';
                elseif ($pvn == 1 && $c==0 && $d==0) return '#D3D3D3';
                elseif ($c==1 && $d==0 && $b==0 && $a==0) return 'rgb(255,128,0)';
                elseif ($c==1 && $d==0 && $a==0) return 'rgb(255,128,0)';
                elseif ($c==0 && $d==1) return '#D3D3D3';
                elseif ($c==1 && $d==1) return '<polyline points="1003.14 117.15 1042.29 117.15" fill="none" class="blink" stroke-width="2" />';
                elseif ($c==1 && $d==1) return '<polyline points="1003.14 117.15 1042.29 117.15" fill="none" class="blink" stroke-width="2" />';
                elseif ($c==1 && $a==1) return '<polyline points="1003.14 117.15 1042.29 117.15" fill="none" class="blink" stroke-width="2" />';
                elseif ($c==1) return 'rgb(255,128,0)';
                elseif ($pvn == 0 && $c==0) return '#D3D3D3';
                else return 'red';
	}		
                
    function sector5($pvn,$c,$d)
    {
           if ($pvn == 1 && $c==0 && $d==0) return '<polyline points="1003.14 117.15 1042.29 117.15" fill="none" class="blink" stroke-width="2" />';
                elseif ($c==0 && $d==1) return 'rgb(0,0,255)';
                elseif ($c==1 && $d==0) return 'rgb(255,128,0)';
                elseif ($d==1 && $c == 1) return 'rgb(255,128,0)';
                elseif ($c==1 && $d==0) return 'rgb(255,128,0)';
                elseif ($c == 0 && $d == 0) return '#D3D3D3';
                else return 'red';
 	}              				

    function sector6($c,$d)
    {
           if ($d==0 && $c==0) return '#D3D3D3';
                elseif ($d==0 && $c==1) return '#D3D3D3';
                elseif ($d==0 && $c==1) return '#D3D3D3';
                elseif ($c==0 && $d==0) return '#D3D3D3';
                elseif ($c==1 && $d==1) return '<polyline points="1003.14 117.15 1042.29 117.15" fill="none" class="blink" stroke-width="2" />';
                elseif ($c == 0 && $d==1) return 'rgb(0,0,255)';
                else return 'red';
 	}

    function sector7($d,$f,$e)
    {
    	   if ($d == 0 && $e==1 && $f==0) return 'rgb(0,0,255)';
        	    elseif ($d == 1 && $e==0) return '#D3D3D3';		
                elseif ($d == 1 && $f == 1 && $e == 1) return 'rgb(0,0,255)';
                elseif ($d == 1 && $e==1 && $f==0) return 'rgb(0,0,255)';
                elseif ($d == 0 && $e==0 && $f==0) return '#D3D3D3';
                elseif ($d == 1 && $f == 0) return '(rgb(0,0,255))';
                elseif ($f == 1 && $e==0) return '#D3D3D3';
                elseif ($f == 1 && $d == 0) return 'rgb(0,0,255)';
                else return 'red';
    					
  	}

    function sector8($a,$b,$f,$e)
    {
           if ($a==1 && $f==1) return '<polyline points="1003.14 117.15 1042.29 117.15" fill="none" class="blink" stroke-width="2" />';
                elseif ($a==0 && $f==0) return '#D3D3D3';
                elseif ($f==1 && $a==0 && $e==1) return 'rgb(0,0,255)';
                elseif ($f==1 && $a==0 && $e==0) return 'rgb(0,0,255)';
                elseif ($f==0 && $a==1) return '#D3D3D3';
                elseif ($f==1 && $a==1) return '<polyline points="1003.14 117.15 1042.29 117.15" fill="none" class="blink" stroke-width="2" />';
                elseif ($f==0) return '#D3D3D3';
                else return 'red';

    }

