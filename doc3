	function sector9($a1,$b1,$c1)
	{
		if ($a1 == 1 && $b1 == 1) return 'rgb(255,128,0)';
			elseif ($a1 == 1 && $b1 == 0 && $c1 == 1) return 'rgb(0,0,255)';
			elseif ($a1 == 1 && $b1 == 0 && $c1 == 0)return 'rgb(255,128,0)';
			elseif ($a1 == 0)return '#D3D3D3';
			else return 'red';
			
	}

	function sector10($a1,$b1,$c1)
	{
		if ($a1 == 1 && $b1 == 1 && $c1 == 1) return '<polyline points="1003.14 117.15 1042.29 117.15" fill="none" class="blink" stroke-width="2" />';
			elseif ($c1 == 1 && $b1 == 1 ) return '<polyline points="1003.14 117.15 1042.29 117.15" fill="none" class="blink" stroke-width="2" />';
			elseif ($a1 == 1 && $b1 == 1 && $c1 == 0) return 'rgb(255,128,0)';
			elseif ($a1 == 1 && $b1 == 0 && $c1 == 1) return '#D3D3D3';
			elseif ($a1 == 1 && $b1 == 0 ) return '#D3D3D3';
			elseif ($a1 == 0 && $b1 == 0 ) return '#D3D3D3';
			elseif ($a1 == 0 && $b1 == 1 ) return 'rgb(255,128,0)';
			else return 'red';
	}

	function sector11($a1,$b1,$c1,$d1)
	{
		if ($a1 == 1 && $b1 == 1 && $c1 == 1) return '<polyline points="1003.14 117.15 1042.29 117.15" fill="none" class="blink" stroke-width="2" />';
			elseif ($c1 == 1 && $d1 == 1 ) return 'rgb(0,0,255)';
			elseif ($c1 == 0 && $d1 == 0 ) return '#D3D3D3';
			elseif ($c1 == 0 && $d1 == 1 ) return '#D3D3D3';
			elseif ($c1 == 1 && $b1 == 1 ) return '<polyline points="1003.14 117.15 1042.29 117.15" fill="none" class="blink" stroke-width="2" />';
			elseif ($c1 == 1 && $d1 == 0 ) return 'rgb(0,0,255)';
			else return 'red';
	}

	function sector41($d1)
	{
		if ($d1 == 1 ) return 'rgb(0,0,255)';
			elseif ($d1 == 0 ) return '#D3D3D3';
			else return 'red';
	}

