	function sector44($l,$i,$o,$j,$j1)
	{
		if ($l == 1 && $i == 1) return '<polyline points="1003.14 117.15 1042.29 117.15" fill="none" class="blink" stroke-width="2" />';
			elseif ($j == 1 && $j1 == 1 && $l == 1) return '#D3D3D3';
			elseif ($j == 1 && $j1 == 1) return '<polyline points="1003.14 117.15 1042.29 117.15" fill="none" class="blink" stroke-width="2" />';
			elseif ($i == 1 && $o == 1 || $j == 1) return 'rgb(255,128,0)';
			elseif ($i == 1 && $o == 1 & $j == 1) return 'rgb(255,128,0)';
			elseif ($o == 0 && $j == 0 && $i == 0) return '#D3D3D3';
			elseif ($o == 0 && $j == 0 && $i == 1) return '#D3D3D3';
			elseif ($o == 1 && $j == 0 && $i == 0) return '#D3D3D3';
			elseif ($o == 1 && $j == 0 && $i == 1) return 'rgb(255,128,0)';
			elseif ($o == 0 && $j == 1 && $i == 0) return '#D3D3D3';
			elseif ($o == 0 && $j == 1 && $i == 1) return 'rgb(255,128,0)';
			else return 'red';
	}

