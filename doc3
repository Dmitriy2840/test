			elseif ($r == 1 && $k == 1 ) return 'rgb(255,128,0)';
			elseif ($k == 1 ) return 'rgb(255,128,0)';
			else return 'red';
	}

	function sector33($j,$j1,$j2)
	{
		if ($j == 0) return '#D3D3D3';
			elseif ($j == 1 && $j2 == 1 ) return '<polyline points="1003.14 117.15 1042.29 117.15" fill="none" class="blink" stroke-width="2" />';
			elseif ($j == 1 && $j1 == 1 ) return 'rgb(0,0,255)';
			elseif ($j == 1 && $j1 == 0 ) return 'rgb(255,128,0)';
			elseif ($j == 1) return 'rgb(255,128,0)';
			else return 'red';
	}

