$y1 = onOff($d34_3,$d34_4);
	
	function sector12($x1, $y1)
	{
		if ($x1 == 1)  return 'rgb(0,0,255)';
			elseif ($x1 == 0 && $y1 == 1)	return '#D3D3D3';
			elseif ($x1 == 0) return '#D3D3D3';
			else return 'red';
	}

	function sector13($x1, $y1)
	{
		if ($y1 == 1)  return 'rgb(0,0,255)';
			elseif ($y1 == 0 && $x1 == 1)	return '#D3D3D3';
			elseif ($y1 == 0) return '#D3D3D3';
			else return 'red';
	}

/////////////////// PVN 3 & 5      !!!!!!

$pvn5 = 0;
$pvn3 = 0;
$x = onOff($d26_3,$d26_4);
$y = onOff($d33_1,$d33_2);
$z = onOff($d26_1,$d26_2);
$w = onOff($d32_5,$d32_6);
$t = onOff($d32_7,$d32_8);
$s = onOff($d25_5,$d25_6);
$v = onOff($d34_5,$d34_6);

	function sector14($x,$y,$z,$w,$t)
	{
		if ($x == 1 && $y == 1)  return 'rgb(255,128,0)';
			elseif ($x == 0 && $y == 1)	return '#D3D3D3';
			elseif ($x == 1 && $z == 1 && $w == 1 && $t == 1) return 'rgb(255,128,0)';
			elseif ($x == 1 && $z == 1 && $w == 1) return 'rgb(0,0,255)';
			elseif ($x == 1 && $z == 1 && $t == 1) return 'rgb(255,128,0)';
			elseif ($x == 1 && $y == 0)	return 'rgb(255,128,0)';
			elseif ($x == 0) return '#D3D3D3';
	}

	function sector15($x,$y,$z)
	{
		if ($y == 1 && $x == 1 && $z == 1)	return '<polyline points="1003.14 117.15 1042.29 117.15" fill="none" class="blink" stroke-width="2" />';
		    elseif ($y == 1 && $x == 1 && $z == 0)	return 'rgb(255,128,0)';
			elseif ($y == 0 && $x == 1 && $z == 1)	return '#D3D3D3';
			elseif ($y == 0 && $x == 1)	return '#D3D3D3';
			elseif ($y == 0) return '#D3D3D3';
			elseif ($y == 1 && $x == 0 && $z == 1)	return '<polyline points="1003.14 117.15 1042.29 117.15" fill="none" class="blink" stroke-width="2" />';
			elseif ($y == 1 && $x == 0 && $z == 0)	return 'rgb(255,128,0)';
			else return 'red';
	}

	function sector16($x,$y,$z,$w,$t,$s)
	{
		if  ($x == 1 && $z == 1 && $w == 1 && $t == 1)	return 'rgb(255,128,0)';
			elseif ($x == 1 && $z == 1 && $t == 1)  return 'rgb(255,128,0)';
			elseif ($x == 1 && $z == 1 && $y == 1)	return '<polyline points="1003.14 117.15 1042.29 117.15" fill="none" class="blink" stroke-width="2" />';
			elseif ($x == 1 && $z == 1 && $w == 1)  return 'rgb(0,0,255)';
			elseif ($x == 1 && $z == 1 && $t == 1)  return 'rgb(255,128,0)';
			elseif ($x == 1 && $z == 0)	return '#D3D3D3';
			elseif ($x == 1 && $y == 1 && $z == 0)	return '#D3D3D3';
			elseif ($x == 0 && $z == 0)	return '#D3D3D3';
			elseif ($x == 0 && $z == 1)	return '<polyline points="1003.14 117.15 1042.29 117.15" fill="none" class="blink" stroke-width="2" />';
			elseif ($x == 1 && $z == 1 && $w == 0 || $t == 0)	return 'rgb(255,128,0)';
			else return 'red';
	}

	function sector17($s,$z,$w,$t)
	{
		if  ($s == 1 && $z == 1 && $w == 1)	return 'rgb(0,0,255)';
			elseif ($s == 1 && $z == 1 && $t == 1 )  return 'rgb(255,128,0)';
			elseif ($s == 1 && $z == 0 && $t == 1 )  return 'rgb(255,128,0)';
			elseif ($s == 1 && $w == 1)  return 'rgb(0,0,255)';
			elseif ($s == 0) return '#D3D3D3';
			elseif ($s == 1 && $t == 0 || $w == 0) return 'rgb(255,128,0)';
 			else return 'red';
	}



	function sector18($s,$z,$w,$t)
	{
		if  ($s == 1 && $w == 1 && $t == 1)	return 'rgb(255,128,0)';
			elseif ($z == 1 && $w == 1 && $t == 1)	return 'rgb(255,128,0)';
			elseif ($s == 1 && $z == 0 && $t == 1 )	return 'rgb(255,128,0)';
			elseif ($s == 1 && $z == 0 && $w == 1)	return 'rgb(0,0,255)';
			elseif ($s == 0 && $z == 1 && $t == 1 )  return 'rgb(255,128,0)';
			elseif ($s == 0 && $z == 1 && $w == 1 )  return 'rgb(0,0,255)';
			elseif ($s == 1 && $w == 1)  return 'rgb(0,0,255)';
			elseif ($s == 0 && $w == 1) return 'rgb(0,0,255)';
			elseif ($s == 0 && $t == 1) return 'rgb(255,128,0)';
			elseif ($s == 0 && $z == 0 && $w == 0 )  return '#D3D3D3';
			elseif ($s == 1 && $t == 0 || $w == 0) return 'rgb(255,128,0)';
 			else return 'red';
	}

	function sector19($s,$z,$w,$t)
	{
		if  ($s == 1 && $w == 1 && $t == 1)	return '<polyline points="1003.14 117.15 1042.29 117.15" fill="none" class="blink" stroke-width="2" />';
			elseif ($z == 1 && $w == 1 && $t == 1)	return '<polyline points="1003.14 117.15 1042.29 117.15" fill="none" class="blink" stroke-width="2" />';
			elseif ($w == 1 && $t == 1)	return '<polyline points="1003.14 117.15 1042.29 117.15" fill="none" class="blink" stroke-width="2" />';
			elseif ($s == 1 && $z == 0 && $t == 1 )	return 'rgb(255,128,0)';
			elseif ($s == 1 && $z == 0 && $w == 1)	return '#D3D3D3';
			elseif ($s == 0 && $z == 1 && $t == 1 )  return 'rgb(255,128,0)';
			elseif ($s == 0 && $z == 1 && $w == 1 )  return '#D3D3D3';
			elseif ($s == 0 && $w == 1) return '#D3D3D3';
			elseif ($s == 0 && $t == 1 && $z == 1 && $w == 0 ) return 'rgb(255,128,0)';
			elseif ($s == 0 && $z == 0 && $t == 1 )  return 'rgb(255,128,0)';
			elseif ($s == 0 && $z == 0 && $w == 0 )  return '#D3D3D3';
			elseif ($s == 0 && $z == 0 && $t == 0 )  return '#D3D3D3';
			elseif ($s == 1 && $z == 1 && $t == 1 )	return 'rgb(255,128,0)';
			elseif ($s == 1 && $z == 1 && $t == 0 && $w == 1)	return '#D3D3D3';
			elseif ($s == 1 && $t == 0)	return '#D3D3D3';
 			else return 'red';
	}


	
	function sector20($s,$z,$w,$t)
	{
		if  ($s == 1 && $w == 1 && $t == 1)	return '<polyline points="1003.14 117.15 1042.29 117.15" fill="none" class="blink" stroke-width="2" />';
			elseif ($z == 1 && $w == 1 && $t == 1)	return '<polyline points="1003.14 117.15 1042.29 117.15" fill="none" class="blink" stroke-width="2" />';
			elseif ($w == 1 && $t == 1)	return '<polyline points="1003.14 117.15 1042.29 117.15" fill="none" class="blink" stroke-width="2" />';
			elseif ($s == 1 && $z == 0 && $t == 1 )	return '#D3D3D3';
			elseif ($s == 1 && $z == 0 && $w == 1)	return 'rgb(0,0,255)';
			elseif ($s == 0 && $z == 1 && $t == 1 )  return '#D3D3D3';
			elseif ($s == 0 && $z == 1 && $w == 1 )  return 'rgb(0,0,255)';
			elseif ($s == 0 && $t == 1) return '#D3D3D3';
			elseif ($s == 0 && $w == 1 && $z == 1 && $t == 0 ) return 'rgb(0,0,255)';
			elseif ($s == 0 && $z == 0 && $w == 1 )  return 'rgb(0,0,255)';
			elseif ($s == 0 && $z == 0 && $t == 0 )  return '#D3D3D3';
			elseif ($s == 1 && $w == 1) return 'rgb(0,0,255)';
			elseif ($s == 1 && $w == 0 && $z == 1 && $t == 1 ) return '#D3D3D3';
			elseif ($s == 1 && $w == 0)	return '#D3D3D3';
 			else return 'red';
	}

	function sector42($v)
	{
		if ($v == 1) return 'rgb(0,0,255)';
			elseif ($v == 0)	return '#D3D3D3';
 			else return 'red';
	}


/////////////////// PVN 4 & 6      !!!!!!

	function sector21($g,$h,$k,$n,$l)
	{
		if ($g == 0) return '#D3D3D3';
			elseif ($n == 1 && $h == 1 && $k == 1) return '<polyline points="1003.14 117.15 1042.29 117.15" fill="none" class="blink" stroke-width="2" />';
			elseif ($n == 1 && $g == 1 && $h == 0 ) return 'rgb(255,128,0)';
			elseif ($n == 1 && $g == 1 && $l == 1 ) return 'rgb(0,0,255)';
			elseif ($n == 1 && $g == 1 && $l == 0) return 'rgb(0,0,255)';
			elseif ($n == 1 && $g == 1) return 'rgb(255,128,0)';
			elseif ($g == 1 && $h == 1 && $k == 1) return 'rgb(255,128,0)';
			elseif ($g == 1 && $h == 1) return 'rgb(0,0,255)';
			elseif ($g == 1 && $h == 0) return 'rgb(255,128,0)';
			elseif ($g == 1 && $k == 0) return 'rgb(255,128,0)';
			elseif ($g == 1 && $k == 1) return 'rgb(255,128,0)';
			else return 'red';
	}





$g = onOff($d25_7,$d25_8);
$h = onOff($d32_9,$d32_10);
//$l = 1;
$m = onOff($d27_1,$d27_2);
$p = onOff($d33_3,$d33_4);
$n = onOff($d26_5,$d26_6);
$k = onOff($d32_11,$d32_12);

	function sector22($g,$l,$k,$n,$h)
	{
		if ($n == 1 && $h == 1 && $k == 1) return '<polyline points="1003.14 117.15 1042.29 117.15" fill="none" class="blink" stroke-width="2" />';
			elseif ($h == 1 && $k == 1) return 'rgb(255,128,0)';
			elseif ($h == 1) return 'rgb(0,0,255)';
			elseif ($k == 1) return 'rgb(255,128,0)';
			elseif ($g == 0 && $n == 1 && $h == 1 ) return 'rgb(0,0,255)';
			elseif ($g == 0 && $n == 1 && $h == 0 && $k == 1) return 'rgb(255,128,0)';
			elseif ($g == 1 && $h == 1) return 'rgb(0,0,255)';
			elseif ($g == 1 && $n == 0 && $h == 1) return 'rgb(0,0,255)';
			elseif ($g == 1 && $n == 0 && $k == 1) return 'rgb(255,128,0)';
			elseif ($g == 1 && $k == 1 && $h == 0) return 'rgb(255,128,0)';
			elseif ($h == 0 && $k == 0 && $g == 1 || $n == 1) return 'rgb(255,128,0)';
			elseif ($h == 0 && $k == 0) return '#D3D3D3';
			else return 'red';
	}

	function sector23($n,$k,$h,$g,$p,$m)
	{
		if ($n == 0) return '#D3D3D3';
			elseif ($n == 1 && $m == 1 ) return '<polyline points="1003.14 117.15 1042.29 117.15" fill="none" class="blink" stroke-width="2" />';
			elseif ($n == 1 && $h == 1 && $k == 1) return '<polyline points="1003.14 117.15 1042.29 117.15" fill="none" class="blink" stroke-width="2" />';
			elseif ($n == 1 && $g == 1 && $h == 1) return 'rgb(0,0,255)';
			elseif ($n == 1 && $g == 1 ) return 'rgb(255,128,0)';
			elseif ($n == 1 && $h == 0 && $k == 0) return 'rgb(255,128,0)';
			elseif ($n == 1 && $h == 1) return 'rgb(0,0,255)';
			elseif ($n == 1 && $k == 1) return 'rgb(255,128,0)';
			else return 'red';
	}

	function sector24($m,$n,$p,$k,$h)
	{
		if ($n == 1 && $m == 1 ) return 'rgb(255,128,0)';
			elseif ($n == 1 && $h == 1) return 'rgb(0,0,255)';
			elseif ($n == 1 && $k == 1) return 'rgb(255,128,0)';
			elseif ($n == 0 && $p == 1) return 'rgb(255,128,0)';
			elseif ($n == 1 && $p == 0) return 'rgb(255,128,0)';
			elseif ($n == 0 && $m == 0) return '#D3D3D3';
			elseif ($m == 1) return 'rgb(255,128,0)';
			else return 'red';
	}

	function sector43($m,$n)
	{
		if ($n == 1 && $m == 1 ) return '<polyline points="1003.14 117.15 1042.29 117.15" fill="none" class="blink" stroke-width="2" />';
			elseif ($m == 0) return '#D3D3D3';
			elseif ($m == 1) return 'rgb(255,128,0)';
			else return 'red';
	}


	
